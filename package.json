{
    "name": "@dozora/url-normalizer",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "build": "tsc && npm test",
        "prepublishOnly": "npm run build",
        "test": "vitest run"
    },
    "files": [
        "dist"
    ],
    "imports": {
        "#*.ts": {
            "@dozora/url-normalizer": "./src/*.ts",
            "default": "./dist/*.js"
        }
    },
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*"
    },
    "simple-git-hooks": {
        "pre-commit": "npm test"
    },
    "dependencies": {
        "normalize-url": "8.1.0"
    },
    "devDependencies": {
        "@types/node": "24.3.3",
        "simple-git-hooks": "2.13.1",
        "typescript": "5.9.2",
        "vitest": "3.2.4"
    }
}
